{
  "name": "dummy-jobs",
  "version": "1.0.0",
  "main": "index.js",
  "description": "",
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "nodemon index.ts",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' index.ts",
    "pretest": "npm run lint && tsc --noEmit",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down"
  },
  "keywords": [],
  "author": "Esteban De la Rosa <gabbanaesteban@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "migrate-mongo": "^8.1.1",
    "mongoose": "^5.10.7",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/morgan": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run lint"
  }
}
